<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.FilterRule" table="OKM_FILTER_RULE" lazy="false">
    <id name="id" column="FR_ID"><generator class="native"/></id>
    <property name="field" column="FR_FIELD" not-null="true"/>
    <property name="operation" column="FR_OPERATION" not-null="true"/>
    <property name="value" column="FR_VALUE" not-null="true"/>
    <property name="active" column="FR_ACTIVE" not-null="true"/>
  </class>
</hibernate-mapping>
