<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/1.7/distro-source/core/src/gwt-module.dtd">
<module rename-to="com.openkm.frontend.Main">

  <!-- Inherit the core Web Toolkit stuff -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name='com.google.gwt.widgetideas.WidgetIdeas'/>
  <inherits name="com.allen_sauer.gwt.log.gwt-log" />
  <inherits name='com.google.gwt.gen2.table.ScrollTable'/>
  
  <!-- Specify the app entry point class -->
  <entry-point class='com.openkm.frontend.client.Main'/>

  <!-- Compile for Firefox only -->
  <!-- <set-property name="user.agent" value="gecko"/> -->

  <!-- Compile debug levels -->
  <extend-property name="log_level" values="DEBUG" />
  <set-property name="log_level" value="DEBUG" />
  <set-property name="log_FirebugLogger" value="DISABLED" />

  <!-- Supress compilation warnings -->
  <set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="true"/>

  <servlet path="/OpenKM/OKMFolderServlet" class="com.openkm.frontend.server.OKMFolderServlet"/>
  <servlet path="/OpenKM/OKMDocumentServlet" class="com.openkm.frontend.server.OKMDocumentServlet"/>
  <servlet path="/OpenKM/OKMDownloadServlet" class="com.openkm.frontend.server.OKMDownloadServlet"/>
  <servlet path="/OpenKM/OKMUploadServlet" class="com.openkm.frontend.server.OKMUploadServlet"/>
  <servlet path="/OpenKM/OKMAuthServlet" class="com.openkm.frontend.server.OKMAuthServlet"/>
  <servlet path="/OpenKM/OKMSearchServlet" class="com.openkm.frontend.server.OKMSearchServlet"/>
  <servlet path="/OpenKM/OKMFileUploadServlet" class="com.openkm.frontend.server.OKMFileUploadServlet"/>
  <servlet path="/OpenKM/OKMPropertyGroupServlet" class="com.openkm.frontend.server.OKMPropertyGroupServlet"/>
  <servlet path="/OpenKM/OKMNotifyServlet" class="com.openkm.frontend.server.OKMNotifyServlet"/>
  <servlet path="/OpenKM/OKMBookmarkServlet" class="com.openkm.frontend.server.OKMBookmarkServlet"/>
  <servlet path="/OpenKM/OKMRepositoryServlet" class="com.openkm.frontend.server.OKMRepositoryServlet"/>
  <servlet path="/OpenKM/OKMDashboardServlet" class="com.openkm.frontend.server.OKMDashboardServlet"/>
  <servlet path="/OpenKM/OKMRepositoryServlet" class="com.openkm.frontend.server.OKMWorkspaceServlet"/>
  <servlet path="/OpenKM/OKMWorkflowServlet" class="com.openkm.frontend.server.OKMWorkflowServlet"/>
  <servlet path="/OpenKM/OKMMailServlet" class="com.openkm.frontend.server.OKMMailServlet"/>
  <servlet path="/OpenKM/OKMGeneralServlet" class="com.openkm.frontend.server.OKMGeneralServlet"/>

</module>
