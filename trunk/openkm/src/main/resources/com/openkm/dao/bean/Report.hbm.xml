<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.Report" table="OKM_REPORT">
    <id name="id" column="RP_ID"><generator class="native"/></id>
    <property name="name" column="RP_NAME" not-null="true" unique="true"/>
    <property name="fileDataBlob" column="RP_FILE_DATA" not-null="true" type="blob"/>
    <property name="fileName" column="RP_FILE_NAME" not-null="true"/>
    <property name="active" column="RP_ACTIVE" not-null="true"/>
  </class>
</hibernate-mapping>
