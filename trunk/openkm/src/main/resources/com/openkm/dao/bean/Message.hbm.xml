<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.Message" table="OKM_MESSAGE">
    <id name="id" column="MSG_ID"><generator class="native"/></id>
    <property name="from" column="MSG_FROM" not-null="true" length="127"/>
    <property name="to" column="MSG_TO" not-null="true" length="127"/>
    <property name="subject" column="MSG_SUBJECT" not-null="true"/>
    <property name="content" column="MSG_CONTENT" type="text"/>
    <property name="sentDate" column="MSG_SENT_DATE" not-null="true"/>
  </class>
</hibernate-mapping>
