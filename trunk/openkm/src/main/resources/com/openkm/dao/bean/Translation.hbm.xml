<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.Translation" table="OKM_TRANSLATION">
  		<id name="id" column="TR_ID"><generator class="native"/></id>
    	<property name="module" column="TR_MODULE" not-null="true" type="text"/>
    	<property name="key" column="TR_KEY" not-null="true" type="text"/>
    	<property name="text" column="TR_TEXT" not-null="true" type="text"/>
  </class>
</hibernate-mapping>