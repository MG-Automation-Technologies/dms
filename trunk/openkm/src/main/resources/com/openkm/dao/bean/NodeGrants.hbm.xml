<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.NodeGrants" table="OKM_NODE_GRANTS">
    <id name="uuid" column="NG_UUID"><generator class="assigned"/></id>
    <property name="usersRead" column="NG_USERS_READ" not-null="true"/>
    <set name="usersRead" table="OKM_NODE_GRANTS_USR_RD" lazy="false">
      <key column="NGUR_ID" not-null="true"/>
      <element column="NGUR_USER" type="string"/>
    </set>
    <set name="usersWrite" table="OKM_NODE_GRANTS_USR_WR" lazy="false">
      <key column="NGUW_ID" not-null="true"/>
      <element column="NGUW_USER" type="string"/>
    </set>
    <set name="usersDelete" table="OKM_NODE_GRANTS_USR_DL" lazy="false">
      <key column="NGUD_ID" not-null="true"/>
      <element column="NGUD_USER" type="string"/>
    </set>
    <set name="usersSecurity" table="OKM_NODE_GRANTS_USR_SG" lazy="false">
      <key column="NGUS_ID" not-null="true"/>
      <element column="NGUS_USER" type="string"/>
    </set>
    <set name="rolesRead" table="OKM_NODE_GRANTS_ROL_RD" lazy="false">
      <key column="NGRR_ID" not-null="true"/>
      <element column="NGRR_ROLE" type="string"/>
    </set>
    <set name="rolesWrite" table="OKM_NODE_GRANTS_ROL_WR" lazy="false">
      <key column="NGRW_ID" not-null="true"/>
      <element column="NGRW_ROLE" type="string"/>
    </set>
    <set name="rolesDelete" table="OKM_NODE_GRANTS_ROL_DL" lazy="false">
      <key column="NGRD_ID" not-null="true"/>
      <element column="NGRD_ROLE" type="string"/>
    </set>
    <set name="rolesSecurity" table="OKM_NODE_GRANTS_ROL_SG" lazy="false">
      <key column="NGRS_ID" not-null="true"/>
      <element column="NGRS_ROLE" type="string"/>
    </set>
  </class>
</hibernate-mapping>
