<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.MailFilter" table="OKM_MAIL_FILTER">
    <id name="id" column="MF_ID"><generator class="native"/></id>
    <property name="path" column="MF_PATH" not-null="true"/>
    <property name="uuid" column="MF_UUID" not-null="true"/>
    <property name="active" column="MF_ACTIVE" not-null="true"/>
    <set name="filterRules" table="OKM_FILTER_RULE" order-by="FR_ID asc" cascade="all-delete-orphan" lazy="false">
      <key column="FR_MAIL_FILTER" not-null="true"/>
      <one-to-many class="com.openkm.dao.bean.FilterRule"/>
    </set>
  </class>
</hibernate-mapping>
