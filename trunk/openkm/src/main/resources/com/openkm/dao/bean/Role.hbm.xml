<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.Role" table="OKM_ROLE">
    <id name="id" column="ROL_ID"><generator class="assigned"/></id>
    <property name="active" column="ROL_ACTIVE" not-null="true"/>
    <set name="users" table="OKM_USER_ROLE" cascade="save-update">
      <key column="UR_ROLE"/>
      <many-to-many column="UR_USER" class="com.openkm.dao.bean.User"/>
    </set>
  </class>
</hibernate-mapping>
