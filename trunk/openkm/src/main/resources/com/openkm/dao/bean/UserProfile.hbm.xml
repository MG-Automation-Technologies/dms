<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.UserProfile" table="OKM_USER_PROFILE">
    <id name="id" column="UP_ID"><generator class="native"/></id>
    <property name="name" column="UP_NAME" not-null="true"/>
    <property name="active" column="UP_ACTIVE" not-null="true"/>

    <!-- MISC -->
    <component name="misc" class="com.openkm.dao.bean.UserProfileMisc">
      <property name="advancedFilters" not-null="true">
        <column name="UP_MISC_ADVANCED_FILTERS" default="false"/>
      </property>
      <property name="userQuota" not-null="true">
        <column name="UP_MISC_USER_QUOTA" default="0"/>
      </property>
    </component>
    
    <!-- CHAT -->
    <component name="chat" class="com.openkm.dao.bean.UserProfileChat">
      <property name="chatEnabled" not-null="true">
        <column name="UP_CHAT_CHAT_ENABLED" default="false"/>
      </property>
      <property name="autoLoginEnabled" not-null="true">
        <column name="UP_CHAT_AUTO_LOGIN_ENABLED" default="false"/>
      </property>
    </component>
    
    <!-- WIZARD -->
    <component name="wizard" class="com.openkm.dao.bean.UserProfileWizard">
      <property name="propertyGroups" not-null="true">
        <column name="UP_WIZARD_PROPERTY_GROUPS" default="''"/>
      </property>
      <property name="keywordsEnabled" not-null="true">
        <column name="UP_WIZARD_KEYWORDS_ENABLED" default="false"/>
      </property>
      <property name="categoriesEnabled" not-null="true">
        <column name="UP_WIZARD_CATEGORIES_ENABLED" default="false"/>
      </property>
    </component>
    
    <!-- STACK -->
    <component name="stack" class="com.openkm.dao.bean.UserProfileStack">
      <property name="categoriesVisible" not-null="true">
        <column name="UP_STACK_CATEGORIES_VISIBLE" default="true"/>
      </property>
      <property name="thesaurusVisible" not-null="true">
        <column name="UP_STACK_THESAURUS_VISIBLE" default="true"/>
      </property>
      <property name="templatesVisible" not-null="true">
        <column name="UP_STACK_TEMPLATES_VISIBLE" default="true"/>
      </property>
      <property name="personalVisible" not-null="true">
        <column name="UP_STACK_PERSONAL_VISIBLE" default="true"/>
      </property>
      <property name="mailVisible" not-null="true">
        <column name="UP_STACK_MAIL_VISIBLE" default="true"/>
      </property>
    </component>
    
    <!-- TAB -->
    <component name="tab" class="com.openkm.dao.bean.UserProfileTab">
      <property name="desktopVisible" not-null="true">
        <column name="UP_TAB_DESKTOP_VISIBLE" default="true"/>
      </property>
      <property name="searchVisible" not-null="true">
        <column name="UP_TAB_SEARCH_VISIBLE" default="true"/>
      </property>
      <property name="dashboardVisible" not-null="true">
        <column name="UP_TAB_DASHBOARD_VISIBLE" default="true"/>
      </property>
    </component>
  
    <!-- DASHBOARD -->
	<component name="dashboard" class="com.openkm.dao.bean.UserProfileDashboard">
      <property name="userVisible" not-null="true">
	    <column name="UP_DASHBOARD_USER_VISIBLE" default="true"/>
	  </property>
	  <property name="mailVisible" not-null="true">
	    <column name="UP_DASHBOARD_MAIL_VISIBLE" default="true"/>
	  </property>
	  <property name="newsVisible" not-null="true">
	    <column name="UP_DASHBOARD_NEWS_VISIBLE" default="true"/>
	  </property>
	  <property name="generalVisible" not-null="true">
	    <column name="UP_DASHBOARD_GENERAL_VISIBLE" default="true"/>
	  </property>
	  <property name="workflowVisible" not-null="true">
	    <column name="UP_DASHBOARD_WORKFLOW_VISIBLE" default="true"/>
	  </property>
	  <property name="keywordsVisible" not-null="true">
	    <column name="UP_DASHBOARD_KEYWORDS_VISIBLE" default="true"/>
	  </property>
	</component>
	
	<!-- MENU -->
	<component name="menu" class="com.openkm.dao.bean.UserProfileMenu">
      <property name="fileVisible" not-null="true">
        <column name="UP_MENU_FILE_VISIBLE" default="true"/>
      </property>
      <property name="editVisible" not-null="true">
        <column name="UP_MENU_EDIT_VISIBLE" default="true"/>
      </property>
      <property name="toolsVisible" not-null="true">
        <column name="UP_MENU_TOOLS_VISIBLE" default="true"/>
      </property>
      <property name="bookmarksVisible" not-null="true">
        <column name="UP_MENU_BOOKMARKS_VISIBLE" default="true"/>
      </property>
      <property name="helpVisible" not-null="true">
        <column name="UP_MENU_HELP_VISIBLE" default="true"/>
      </property>
    </component>
		
	<!-- ACTION -->
	<component name="action" class="com.openkm.dao.bean.UserProfileAction">
	  <property name="createFolderVisible" not-null="true">
	    <column name="UP_ACTION_CREATE_FOLDER_VISIBLE" default="true"/>
	  </property>
	  <property name="findFolderVisible" not-null="true">
	    <column name="UP_ACTION_FIND_FOLDER_VISIBLE" default="true"/>
	  </property>
	   <property name="goFolderVisible" not-null="true">
	    <column name="UP_ACTION_GO_FOLDER_VISIBLE" default="true"/>
	  </property>
	  <property name="downloadVisible" not-null="true">
	    <column name="UP_ACTION_DOWNLOAD_VISIBLE" default="true"/>
	  </property>
	  <property name="downloadPdfVisible" not-null="true">
	    <column name="UP_ACTION_DOWNLOAD_PDF_VISIBLE" default="true"/>
	  </property>
	  <property name="lockVisible" not-null="true">
	    <column name="UP_ACTION_LOCK_VISIBLE" default="true"/>
	  </property>
	  <property name="unlockVisible" not-null="true">
	    <column name="UP_ACTION_UNLOCK_VISIBLE" default="true"/>
	  </property>
	  <property name="addDocumentVisible" not-null="true">
	    <column name="UP_ACTION_ADD_DOCUMENT_VISIBLE" default="true"/>
	  </property>
	  <property name="checkInVisible" not-null="true">
	    <column name="UP_ACTION_CHECK_IN_VISIBLE" default="true"/>
	  </property>
	  <property name="checkOutVisible" not-null="true">
	    <column name="UP_ACTION_CHECK_OUT_VISIBLE" default="true"/>
	  </property>
	  <property name="cancelCheckOutVisible" not-null="true">
	    <column name="UP_ACTION_CANCEL_CHECK_OUT_VISIBLE" default="true"/>
	  </property>
	  <property name="deleteVisible" not-null="true">
	    <column name="UP_ACTION_DELETE_VISIBLE" default="true"/>
	  </property>
	  <property name="addPropertyGroupVisible" not-null="true">
	    <column name="UP_ACTION_ADD_PROPERTY_GROUP_VISIBLE" default="true"/>
	  </property>
	  <property name="removePropertyGroupVisible" not-null="true">
	    <column name="UP_ACTION_REMOVE_PROPERTY_GROUP_VISIBLE" default="true"/>
	  </property>
	  <property name="startWorkflowVisible" not-null="true">
	    <column name="UP_ACTION_START_WORKFLOW_VISIBLE" default="true"/>
	  </property>
	  <property name="addSubscriptionVisible" not-null="true">
	    <column name="UP_ACTION_ADD_SUBSCRIPTION_VISIBLE" default="true"/>
	  </property>
	  <property name="removeSubscriptionVisible" not-null="true">
	    <column name="UP_ACTION_REMOVE_SUBSCRIPTION_VISIBLE" default="true"/>
	  </property>
	  <property name="refreshVisible" not-null="true">
	    <column name="UP_ACTION_REFRESH_VISIBLE" default="true"/>
	  </property>
	  <property name="goHomeVisible" not-null="true">
	    <column name="UP_ACTION_GO_HOME_VISIBLE" default="true"/>
	  </property>
	  <property name="setHomeVisible" not-null="true">
	    <column name="UP_ACTION_SET_HOME_VISIBLE" default="true"/>
	  </property>
	  <property name="scannerVisible" not-null="true">
	    <column name="UP_ACTION_SCANNER_VISIBLE" default="true"/>
	  </property>
	  <property name="uploaderVisible" not-null="true">
	    <column name="UP_ACTION_UPLOADER_VISIBLE" default="true"/>
	  </property>
	  <property name="renameVisible" not-null="true">
	    <column name="UP_ACTION_RENAME_VISIBLE" default="true"/>
	  </property>
	  <property name="copyVisible" not-null="true">
	    <column name="UP_ACTION_COPY_VISIBLE" default="true"/>
	  </property>
	  <property name="moveVisible" not-null="true">
	    <column name="UP_ACTION_MOVE_VISIBLE" default="true"/>
	  </property>
	  <property name="purgeVisible" not-null="true">
	    <column name="UP_ACTION_PURGE_VISIBLE" default="true"/>
	  </property>
	  <property name="purgeTrashVisible" not-null="true">
	    <column name="UP_ACTION_PURGE_TRASH_VISIBLE" default="true"/>
	  </property>
	  <property name="restoreVisible" not-null="true">
	    <column name="UP_ACTION_RESTORE_VISIBLE" default="true"/>
	  </property>
	  <property name="addBookmarkVisible" not-null="true">
	    <column name="UP_ACTION_ADD_BOOKMARK_VISIBLE" default="true"/>
	  </property>
	  <property name="manageBookmarksVisible" not-null="true">
	    <column name="UP_ACTION_MANAGE_BOOKMARKS_VISIBLE" default="true"/>
	  </property>
	  <property name="exportVisible" not-null="true">
	    <column name="UP_ACTION_EXPORT_VISIBLE" default="true"/>
	  </property>
	  <property name="createFromTemplateVisible" not-null="true">
	    <column name="UP_ACTION_CREATE_FROM_TEMPLATE_VISIBLE" default="true"/>
	  </property>
	  <property name="sendDocumentLinkVisible" not-null="true">
	    <column name="UP_ACTION_SEND_DOCUMENT_LINK_VISIBLE" default="true"/>
	  </property>
	  <property name="sendDocumentAttachmentVisible" not-null="true">
	    <column name="UP_ACTION_SEND_DOCUMENT_ATTACHMENT_VISIBLE" default="true"/>
	  </property>
	  <property name="changeSkinVisible" not-null="true">
	    <column name="UP_ACTION_CHANGE_SKIN_VISIBLE" default="true"/>
	  </property>
	  <property name="debugVisible" not-null="true">
	    <column name="UP_ACTION_DEBUG_VISIBLE" default="true"/>
	  </property>
	  <property name="administrationVisible" not-null="true">
	    <column name="UP_ACTION_ADMINISTRATION_VISIBLE" default="true"/>
	  </property>
	  <property name="helpVisible" not-null="true">
	    <column name="UP_ACTION_HELP_VISIBLE" default="true"/>
	  </property>
	  <property name="documentationVisible" not-null="true">
	    <column name="UP_ACTION_DOCUMENTATION_VISIBLE" default="true"/>
	  </property>
	  <property name="bugTrackingVisible" not-null="true">
	    <column name="UP_ACTION_BUG_TRACKING_VISIBLE" default="true"/>
	  </property>
	  <property name="supportVisible" not-null="true">
	    <column name="UP_ACTION_SUPPORT_VISIBLE" default="true"/>
	  </property>
	  <property name="forumVisible" not-null="true">
	    <column name="UP_ACTION_FORUM_VISIBLE" default="true"/>
	  </property>
	  <property name="changelogVisible" not-null="true">
	    <column name="UP_ACTION_CHANGELOG_VISIBLE" default="true"/>
	  </property>
	  <property name="webSiteVisible" not-null="true">
	    <column name="UP_ACTION_WEB_SITE_VISIBLE" default="true"/>
	  </property>
	  <property name="aboutVisible" not-null="true">
	    <column name="UP_ACTION_ABOUT_VISIBLE" default="true"/>
	  </property>
    </component>
    
  </class>
</hibernate-mapping>
