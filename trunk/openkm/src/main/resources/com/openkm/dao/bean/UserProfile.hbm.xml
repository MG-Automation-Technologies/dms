<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.UserProfile" table="OKM_USER_PROFILE">
    <id name="id" column="UP_ID"><generator class="native"/></id>
    <property name="name" column="UP_NAME" not-null="true"/>
    <property name="active" column="UP_ACTIVE" not-null="true"/>
    
    <property name="advancedFilters" not-null="true">
      <column name="UP_ADVANCED_FILTERS" default="false"/>
    </property>
    
    <property name="userQuotaSize" not-null="true">
      <column name="UP_USER_QUOTA_SIZE" default="0"/>
    </property>
    <property name="userQuotaEnabled" not-null="true">
      <column name="UP_USER_QUOTA_ENABLED" default="false"/>
    </property>
    
    <property name="wizardPropertyGroups" not-null="true">
      <column name="UP_WIZARD_PROPERTY_GROUPS" default="false"/>
    </property>
    <property name="wizardKeywords" not-null="true">
      <column name="UP_WIZARD_KEYWORDS" default="false"/>
    </property>
    <property name="wizardCategories" not-null="true">
      <column name="UP_WIZARD_CATEGORIES" default="false"/>
    </property>
    
    <property name="chatEnabled" not-null="true">
      <column name="UP_CHAT_ENABLED" default="true"/>
    </property>
    <property name="chatAutoLogin" not-null="true">
      <column name="UP_CHAT_AUTO_LOGIN" default="false"/>
    </property>
    
    <property name="stackCategoriesVisible" not-null="true">
      <column name="UP_STACK_CATEGORIES_VISIBLE" default="true"/>
    </property>
    <property name="stackThesaurusVisible" not-null="true">
      <column name="UP_STACK_THESAURUS_VISIBLE" default="true"/>
    </property>
    <property name="stackPersonalVisible" not-null="true">
      <column name="UP_STACK_PERSONAL_VISIBLE" default="true"/>
    </property>
    <property name="stackMailVisible" not-null="true">
      <column name="UP_STACK_MAIL_VISIBLE" default="true"/>
    </property>
    
    <property name="menuEditVisible" not-null="true">
      <column name="UP_MENU_EDIT_VISIBLE" default="true"/>
    </property>
    <property name="menuToolsVisible" not-null="true">
      <column name="UP_MENU_TOOLS_VISIBLE" default="true"/>
    </property>
    <property name="menuBookmarksVisible" not-null="true">
      <column name="UP_MENU_BOOKMARKS_VISIBLE" default="true"/>
    </property>
    <property name="menuHelpVisible" not-null="true">
      <column name="UP_MENU_HELP_VISIBLE" default="true"/>
    </property>
    
    <property name="tabDesktopVisible" not-null="true">
      <column name="UP_TAB_DESKTOP_VISIBLE" default="true"/>
    </property>
    <property name="tabSearchVisible" not-null="true">
      <column name="UP_TAB_SEARCH_VISIBLE" default="true"/>
    </property>
    <property name="tabDashboardVisible" not-null="true">
      <column name="UP_TAB_DASHBOARD_VISIBLE" default="true"/>
    </property>
    
    <property name="dashboardUserVisible" not-null="true">
      <column name="UP_DASHBOARD_USER_VISIBLE" default="true"/>
    </property>
    <property name="dashboardMailVisible" not-null="true">
      <column name="UP_DASHBOARD_MAIL_VISIBLE" default="true"/>
    </property>
    <property name="dashboardNewsVisible" not-null="true">
      <column name="UP_DASHBOARD_NEWS_VISIBLE" default="true"/>
    </property>
    <property name="dashboardGeneralVisible" not-null="true">
      <column name="UP_DASHBOARD_GENERAL_VISIBLE" default="true"/>
    </property>
    <property name="dashboardWorkflowVisible" not-null="true">
      <column name="UP_DASHBOARD_WORKFLOW_VISIBLE" default="true"/>
    </property>
    <property name="dashboardKeywordsVisible" not-null="true">
      <column name="UP_DASHBOARD_KEYWORDS_VISIBLE" default="true"/>
    </property>
  </class>
</hibernate-mapping>
