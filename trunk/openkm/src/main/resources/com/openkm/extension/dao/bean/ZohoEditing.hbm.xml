<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.extension.dao.bean.ZohoEditing" table="OKM_ZOHO_EDITING">
    <id name="id" column="ZED_ID"><generator class="native"/></id>
    <property name="user" column="ZED_USER" not-null="true" length="127"/>
    <property name="uuid" column="ZED_UUID" length="127"/>
    <property name="tmp" column="ZED_TMP" length="255"/>
  </class>
</hibernate-mapping>
