<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.extension.dao.bean.ExtensionMetadataType" table="OKM_EXTENSION_MD_TYPE">
    <id name="id" column="EMT_ID"><generator class="native"/></id>
    <property name="table" column="EMT_TABLE" not-null="true" length="32"/>
    <property name="virtualColumn" column="EMT_VIRTUAL_COLUMN" not-null="true" length="32"/>
    <property name="realColumn" column="EMT_REAL_COLUMN" not-null="true" length="6"/>
    <property name="type" column="EMT_TYPE" not-null="true" length="32"/>
  </class>

  <class name="com.openkm.extension.dao.bean.ExtensionMetadataValue" table="OKM_EXTENSION_MD_VALUE">
    <id name="id" column="EMV_ID"><generator class="native"/></id>
    <property name="table" column="EMV_TABLE" not-null="true" length="32"/>
    <property name="pk" column="EMV_PK" not-null="true"/>
    <property name="col00" column="EMV_COL00" />
    <property name="col01" column="EMV_COL01" />
    <property name="col02" column="EMV_COL02" />
    <property name="col03" column="EMV_COL03" />
    <property name="col04" column="EMV_COL04" />
    <property name="col05" column="EMV_COL05" />
    <property name="col06" column="EMV_COL06" />
    <property name="col07" column="EMV_COL07" />
    <property name="col08" column="EMV_COL08" />
    <property name="col09" column="EMV_COL09" />
  </class>
</hibernate-mapping>
