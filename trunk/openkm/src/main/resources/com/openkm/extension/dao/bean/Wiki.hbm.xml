<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.extension.dao.bean.WikiPage" table="OKM_WIKI_PAGE">
    <id name="id" column="WKP_ID"><generator class="native"/></id>
    <property name="date" column="WKP_DATE" not-null="true"/>
    <property name="user" column="WKP_USER" not-null="true" length="127"/>
    <property name="node" column="WKP_NODE" length="127"/>
    <property name="content" column="KWP_CONTENT" type="text"/>
  </class>
</hibernate-mapping>
