<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="com.openkm.backend.Main">

  <!-- Inherit the core Web Toolkit stuff -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name='com.googlecode.gchart.GChart'/>

  <!-- Specify the app entry point class -->
  <entry-point class='com.openkm.backend.client.Main'/>

  <!-- Compile for Firefox only -->
  <!-- <set-property name="user.agent" value="gecko"/> -->

  <!-- Supress compilation warnings -->
  <set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="true"/>

  <servlet path="/OpenKM/OKMSearchServletAdmin" class="com.openkm.backend.server.OKMSearchServletAdmin"/>
  <servlet path="/OpenKM/OKMGeneralUtilsServletAdmin" class="com.openkm.backend.server.OKMGeneralUtilsServletAdmin"/>
  <servlet path="/OpenKM/OKMPropertyGroupServletAdmin" class="com.openkm.backend.server.OKMPropertyGroupServletAdmin"/>
  <servlet path="/OpenKM/OKMSearchServletAdmin" class="com.openkm.backend.server.OKMSearchServletAdmin"/>
  <servlet path="/OpenKM/OKMUserServletAdmin" class="com.openkm.backend.server.OKMUserServletAdmin"/>
  <servlet path="/OpenKM/OKMFolderServletAdmin" class="com.openkm.backend.server.OKMFolderServletAdmin"/>
  <servlet path="/OpenKM/OKMRepositoryServletAdmin" class="com.openkm.backend.server.OKMRepositoryServletAdmin"/>
  <servlet path="/OpenKM/OKMStatsServletAdmin" class="com.openkm.backend.server.OKMStatsServletAdmin"/>
  <servlet path="/OpenKM/OKMWorkflowServletAdmin" class="com.openkm.backend.server.OKMWorkflowServletAdmin"/>

</module>
