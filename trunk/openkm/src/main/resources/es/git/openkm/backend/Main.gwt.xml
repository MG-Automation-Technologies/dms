<module rename-to="es.git.openkm.backend.Main">

  <!-- Inherit the core Web Toolkit stuff.                  -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name='com.googlecode.gchart.GChart'/>

  <!-- Specify the app entry point class.                   -->
  <entry-point class='es.git.openkm.backend.client.Main'/>

  <!-- Compile for Firefox only -->
  <!-- <set-property name="user.agent" value="gecko"/> -->

  <!-- Supress compilation warnings -->
  <set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="true"/>

  <servlet path="/OpenKM/OKMSearchServletAdmin" class="es.git.openkm.backend.server.OKMSearchServletAdmin"/>
  <servlet path="/OpenKM/OKMGeneralUtilsServletAdmin" class="es.git.openkm.backend.server.OKMGeneralUtilsServletAdmin"/>
  <servlet path="/OpenKM/OKMPropertyGroupServletAdmin" class="es.git.openkm.backend.server.OKMPropertyGroupServletAdmin"/>
  <servlet path="/OpenKM/OKMSearchServletAdmin" class="es.git.openkm.backend.server.OKMSearchServletAdmin"/>
  <servlet path="/OpenKM/OKMUserServletAdmin" class="es.git.openkm.backend.server.OKMUserServletAdmin"/>
  <servlet path="/OpenKM/OKMFolderServletAdmin" class="es.git.openkm.backend.server.OKMFolderServletAdmin"/>
  <servlet path="/OpenKM/OKMRepositoryServletAdmin" class="es.git.openkm.backend.server.OKMRepositoryServletAdmin"/>
  <servlet path="/OpenKM/OKMStatsServletAdmin" class="es.git.openkm.backend.server.OKMStatsServletAdmin"/>
  <servlet path="/OpenKM/OKMWorkflowServletAdmin" class="es.git.openkm.backend.server.OKMWorkflowServletAdmin"/>

</module>
